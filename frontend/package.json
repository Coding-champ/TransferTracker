    "axios": "^1.3.4",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "date-fns": "^2.29.3",
    "lucide-react": "0.533.0",
    "postcss": "^8.5.6",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^7.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.5.0",
